.scroll-view_H{
  
  box-sizing: border-box;
}
.section-group{
  box-sizing: border-box;
  padding: 10px 10px;
}
.section{
  display: flex;
  justify-content: space-between;
  margin-bottom:20px;
  
}
.placeholder{
  color: #888;
}
label{
  color: #444;
  margin-right:5px;
}
input{
  flex:1;
} 
[disabled]{
  color: #999;
}

button{
  margin-bottom: 10px;
}
.section-title{
  padding: 10px 5px;
  color: #999;
  
  background: #f6f8f9;
}
.haveTag{
  background: #eee;
  width: 75rpx;
  color: #999;
  text-align: center;
  border-radius: 5rpx;
}
.have{
  background: #00eb83;
  color: #fff;
}
.icon-call{
  background-image:url('data:image/png;base64,');
}
.image-list{
  flex-wrap: wrap;
  justify-content:flex-start;
  align-items: center;
  align-content: flex-start;
}
.pic-wrap{
  position: relative;
   margin-right: 20rpx;
  margin-bottom: 10px; 
  width: 150rpx;
  height: 150rpx;
  border: solid 1px #bfbfbf;
}
.pic-wrap:nth-child(4n){
   margin-right: 0;
}
.pic{
 width: 100%;
 height: 100%;
}
